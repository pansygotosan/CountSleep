<h1>Sleeps#new</h1>
<p>Find me in app/views/sleeps/new.html.erb</p>
<div style="width: 300px;">
  <%= form_for(@sleep) do |f| %>
  <div>
    <%= f.label :寝た時間 %>
    <%= f.datetime_select :start, class: 'form-control' %>
  </div>
  <div>
    <%= f.label :起きた時間 %>
    <%= f.datetime_select :finish, class: 'form-control' %>
  </div>
  <div>
    <%= f.label :睡眠の種類 %>
    <%= f.text_field :type, size: 50 %>
  </div>
    <%= f.submit 'Submit' %>
  <% end %>
</div>
<p><%= link_to '一覧に戻る', sleeps_url %></p>
